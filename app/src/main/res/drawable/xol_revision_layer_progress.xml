<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 总进度的颜色 -->
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="20dp"/>
            <solid android:color="#FCE7BA" />
        </shape>
    </item>
    <!-- 缓存的颜色 -->
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="20dp" />
                <gradient
                    android:endColor="#FCE7BA"
                    android:startColor="#FCE7BA"></gradient>
            </shape>
        </clip>
    </item>

    <!-- 当前进度的颜色 -->
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="20dp" />
                <gradient
                    android:endColor="#F4AE1A"
                    android:startColor="#F4AE1A"></gradient>
            </shape>
        </clip>
    </item>
</layer-list>